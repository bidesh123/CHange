= form_for @set_user, :set_user, :url => {:action=>"update", :controller=>"admin/set_users", :id => @set_user.id}, :html => {:method => "put"} do |f|
  .field
    = label_tag "First Name:"
    = f.text_field :first_name
  .field
    = label_tag "last Name:"
    = f.text_field :last_name
  .field
    = label_tag "Email:"
    = f.text_field :email
  .actions
    = f.submit 'Save'
